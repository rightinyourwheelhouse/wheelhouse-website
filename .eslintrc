{
  "extends": [
    "eslint-config-daren"
  ],
  "settings": {
    "react": {
      "version": "detect"
    },
    "import/resolver": {
      "alias": [
        [
          "~api",
          "./src/api"
        ],
        [
          "~components",
          "./src/components"
        ],
        [
          "~data",
          "./src/data"
        ],
        [
          "~hooks",
          "./src/hooks"
        ],
        [
          "~images",
          "./src/images"
        ],
        [
          "~layouts",
          "./src/layouts"
        ],
        [
          "~modules",
          "./src/modules"
        ],
        [
          "~pages",
          "./src/pages"
        ],
        [
          "~styles",
          "./src/styles"
        ],
        [
          "~styles",
          "./src/styles"
        ],
        [
          "~templates",
          "./src/templates"
        ],
        [
          "~utils",
          "./src/utils"
        ]
      ]
    }
  }
}